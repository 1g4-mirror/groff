.\" NS Nx user macro
.\" NS   print NetBSD
.\" NS
.\" NS modifies:
.\" NS   doc-curr-font, doc-curr-size
.\" NS
.\" NS local variable:
.\" NS   doc-str-Nx, doc-str-Nx1, doc-str-Nx2
.\" NS
.\" NS width register `Nx' defined in doc-common
.
.de Nx
.  nr doc-curr-font \n[.f]
.  nr doc-curr-size \n[.s]
.  ds doc-str-Nx \f[\n[doc-curr-font]]\s[\n[doc-curr-size]]
.  ds doc-str-Nx1 \~
.  ds doc-str-Nx2 "\$1
.
.  ie !\n[.$] \
.    ds doc-str-Nx1
.  el \{ .ie "\$1"0.8a" .ds doc-str-Nx2 0.8A
.  el \{ .ie "\$1"0.9a" .ds doc-str-Nx2 0.9A
.  el \{ .ie "\$1"1.0a" .ds doc-str-Nx2 1.0A
.  el \{ .ie "\$1"1.2a" .ds doc-str-Nx2 1.2A
.  el    .if "\$1"1.2b" .ds doc-str-Nx2 1.2B
.  \}\}\}\}
.
.  nop \*[doc-Tn-font-size]NetBSD\*[doc-str-Nx1]\*[doc-str-Nx2]\*[doc-str-Nx]\$2
