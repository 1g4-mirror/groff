.\" NS At user macro
.\" NS   print AT&T UNIX
.\" NS
.\" NS modifies:
.\" NS   doc-curr-font, doc-curr-size
.\" NS
.\" NS local variable:
.\" NS   doc-str-At
.\" NS
.\" NS width register `At' defined in doc-common
.
.de At
.  nr doc-curr-font \n[.f]
.  nr doc-curr-size \n[.s]
.  ds doc-str-At \f[\n[doc-curr-font]]\s[\n[doc-curr-size]]
.
.  if "\$1"32v" \
.    nop \&Version\~32V \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"v1" \
.    nop \&Version\~1 \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"v2" \
.    nop \&Version\~2 \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"v3" \
.    nop \&Version\~3 \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"v4" \
.    nop \&Version\~4 \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"v5" \
.    nop \&Version\~5 \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"v6" \
.    nop \&Version\~6 \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"v7" \
.    nop \&Version\~7 \*[doc-Tn-font-size]AT&T UNIX\*[doc-str-At]\c
.  if "\$1"V" \{\
.    nop \*[doc-Tn-font-size]AT&T\*[doc-str-At] System\~V
.    nop \*[doc-Tn-font-size]UNIX\*[doc-str-At]\c
.  \}
.  if "\$1"V.1" \{\
.    nop \*[doc-Tn-font-size]AT&T\*[doc-str-At] System\~V.1
.    nop \*[doc-Tn-font-size]UNIX\*[doc-str-At]\c
.  \}
.  if "\$1"V.2" \{\
.    nop \*[doc-Tn-font-size]AT&T\*[doc-str-At] System\~V.2
.    nop \*[doc-Tn-font-size]UNIX\*[doc-str-At]\c
.  \}
.  if "\$1"V.4" \{\
.    nop \*[doc-Tn-font-size]AT&T\*[doc-str-At] System\~V.4
.    nop \*[doc-Tn-font-size]UNIX\*[doc-str-At]\c
.  \}
.
.  ie (\n[.$] == 2) \
.    nop \&\$2
.  el \
.    nop \&
..
.
.
