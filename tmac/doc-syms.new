.\" NS Ux user macro
.\" NS   print UNIX
.\" NS
.\" NS modifies:
.\" NS   doc-curr-font, doc-curr-size, doc-out-string, doc-arg-ptr
.\" NS
.\" NS local variable:
.\" NS   doc-str-Ux
.\" NS
.\" NS width register `Ux' defined in doc-common
.
.de Ux
.  nr doc-curr-font \n[.f]
.  nr doc-curr-size \n[.s]
.  ds doc-str-Ux \f[\n[doc-curr-font]]\s[\n[doc-curr-size]]
.
.  if !\n[doc-arg-limit] \
.    if \n[.$] \
.      doc-parse-args \$@
.
.  ds doc-arg\n[doc-arg-ptr] \*[doc-Tn-font-size]UNIX\*[doc-str-Ux]
.  ds doc-type\n[doc-arg-ptr] string
.  ds doc-space\n[doc-arg-ptr] "\*[doc-space]
.
.  doc-print-recursive
..
.
.
.\" NS Bx user macro
.\" NS   print BSD (fix smaller nroff version)
.\" NS
.\" NS modifies:
.\" NS   doc-curr-font, doc-curr-size, doc-arg-ptr, doc-out-string
.\" NS
.\" NS local variable:
.\" NS   doc-str-Bx, doc-reg-Bx
.\" NS
.\" NS width register `Bx' defined in doc-common
.
.de Bx
.  nr doc-curr-font \n[.f]
.  nr doc-curr-size \n[.s]
.  ds doc-str-Bx \f[\n[doc-curr-font]]\s[\n[doc-curr-size]]
.
.  if !\n[doc-arg-limit] \{\
.    ie \n[.$] \
.      doc-parse-args \$@
.    el \
.      nop \*[doc-Tn-font-size]BSD\*[doc-str-Bx]
.  \}
.
.  if "\$1"-alpha" \{\
.    nop \&currently in alpha test.
.    doc-clean-vectors
.  \}
.  if "\$1"-beta" \{\
.    nop \&currently in beta test.
.    doc-clean-vectors
.  \}
.  if "\$1"-devel" \{\
.    nop \&currently under development.
.    doc-clean-vectors
.  \}
.
.  nr doc-arg-ptr +1
.  if (\n[doc-arg-limit] >= \n[doc-arg-ptr]) \{\
.    ie "\*[doc-type\n[doc-arg-ptr]]"string" \{\
.      as doc-out-string \&\*[doc-arg\n[doc-arg-ptr]]\*[doc-Tn-font-size]
.      as doc-out-string BSD\*[doc-str-Bx]
.
.      ie (\n[doc-arg-limit] > \n[doc-arg-ptr]) \{\
.        nr doc-reg-Bx (\n[doc-arg-ptr] + 1)
.        ie "\*[doc-type\n[doc-reg-Bx]]"string" \{\
.          nr doc-arg-ptr +1
.          ie        "\*[doc-arg\n[doc-reg-Bx]]"Reno" \
.            as doc-out-string \-Reno
.          el \{ .ie "\*[doc-arg\n[doc-reg-Bx]]"reno" \
.            as doc-out-string \-Reno
.          el \{ .ie "\*[doc-arg\n[doc-reg-Bx]]"Tahoe" \
.            as doc-out-string \-Tahoe
.          el \{ .ie "\*[doc-arg\n[doc-reg-Bx]]"tahoe" \
.            as doc-out-string \-Tahoe
.          el \{ .ie "\*[doc-arg\n[doc-reg-Bx]]"Lite" \
.            as doc-out-string \-Lite
.          el \{ .ie "\*[doc-arg\n[doc-reg-Bx]]"lite" \
.            as doc-out-string \-Lite
.          el \{ .ie "\*[doc-arg\n[doc-reg-Bx]]"Lite2" \
.            as doc-out-string \-Lite2
.          el \{ .ie "\*[doc-arg\n[doc-reg-Bx]]"lite2" \
.            as doc-out-string \-Lite2
.          el \
.            nr doc-arg-ptr -1
.          \}\}\}\}\}\}\}
.
.          ie (\n[doc-arg-limit] > \n[doc-arg-ptr]) \{\
.            nr doc-arg-ptr +1
.            doc-print-recursive
.          \}
.          el \
.            doc-print-and-reset
.        \}
.        el \{\
.          nr doc-arg-ptr +1
.          doc-print-recursive
.        \}
.      \}
.      el \
.        doc-print-and-reset
.    \}
.    el \{\
.      as doc-out-string \*[doc-Tn-font-size]BSD\*[doc-str-Bx]
.      doc-print-recursive
.    \}
.  \}
..
.
.
